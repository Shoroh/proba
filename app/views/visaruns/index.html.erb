<p id="notice"><%= notice %></p>

<h1>Listing Visaruns</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @visaruns.each do |visarun| %>
      <tr>
        <td><%= link_to 'Show', visarun %></td>
        <td><%= link_to 'Edit', edit_visarun_path(visarun) %></td>
        <td><%= link_to 'Destroy', visarun, method: :delete, data: { confirm: 'Are you sure?' } %></td>

      </tr>
    <% end %>
  </tbody>
</table>

<br>
<%= button_to "Cancel my account", registration_path(:user), data: { confirm: "Are you sure?"  , method: :delete }, class: 'btn btn-warning' %>
<%= link_to 'New Visarun', new_visarun_path %>
